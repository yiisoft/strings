{
    "name": "yiisoft/strings",
    "type": "library",
    "description": "Yii - String Helper",
    "keywords": [
        "yii",
        "helper",
        "string"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/strings",
        "issues": "https://github.com/yiisoft/strings/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "infection/infection": "^0.16.3",
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Strings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Strings\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
